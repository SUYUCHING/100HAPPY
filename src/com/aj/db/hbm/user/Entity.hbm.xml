<?xml version="1.0" encoding="UTF-8"?> 

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.aj.db.domain.user.Entity" table="t_entity" >
    <id name="id" column="f_id" type="long" >
     <generator class="sequence">
		  <param name="sequence">seq_entity_id</param>
       </generator>
    </id>
    
	<property name="name" type="string"  column="f_name" />
	<property name="code" type="string"  column="f_code" />
	
	
  </class>
</hibernate-mapping>